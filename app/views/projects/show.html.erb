<div class="center jumbotron">
<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @project.project_name %>
</p>

<p>
  <strong>Organization Name:</strong>
  <%= @project.get_organization_name%>
</p>

<p>
  <strong>Employee Names:</strong>
  <% if @project.get_employee_name.nil? %>
  No Employees Yet.</br> <%= link_to "Add Employee", new_employee_path ,class: "btn btn-primary"%>
  <% else %>
  <%= @project.get_employee_name %>
  <% end%>
</p>

<%= link_to 'Edit', edit_project_path(@project) , class: "btn btn-primary"%> |
<%= link_to 'Back', projects_path , class: "btn btn-primary"%>
</div>